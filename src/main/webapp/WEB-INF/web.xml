<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>legalCustomerServlet</servlet-name>
        <servlet-class>ir.dotin.presentation.AddLegalCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>realCustomerServlet</servlet-name>
        <servlet-class>ir.dotin.presentation.AddRealCustomerServlet</servlet-class>
    </servlet>
    <!--<servlet>-->
        <!--<servlet-name>realCustomerServlet</servlet-name>-->
        <!--<servlet-class>ir.dotin.presentation.DeleteRealCustomerServlet</servlet-class>-->
    <!--</servlet>-->
    <servlet-mapping>
        <servlet-name>legalCustomerServlet</servlet-name>
        <url-pattern>/AddLegalCustomer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>realCustomerServlet</servlet-name>
        <url-pattern>/AddRealCustomer</url-pattern>
    </servlet-mapping>
</web-app>
