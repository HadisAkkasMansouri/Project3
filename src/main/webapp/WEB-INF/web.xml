<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>AddLegalCustomerServlet</servlet-name>
        <servlet-class>ir.dotin.presentation.AddLegalCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddRealCustomerServlet</servlet-name>
        <servlet-class>ir.dotin.presentation.AddRealCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteRealCustomerServlet</servlet-name>
        <servlet-class>ir.dotin.presentation.DeleteRealCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteLegalCustomerServlet</servlet-name>
        <servlet-class>ir.dotin.presentation.DeleteLegalCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateLegalCustomerServlet</servlet-name>
        <servlet-class>ir.dotin.presentation.UpdateLegalCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateRealCustomerServlet</servlet-name>
        <servlet-class>ir.dotin.presentation.UpdateRealCustomerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddLegalCustomerServlet</servlet-name>
        <url-pattern>/AddLegalCustomer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddRealCustomerServlet</servlet-name>
        <url-pattern>/AddRealCustomer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteRealCustomerServlet</servlet-name>
        <url-pattern>/DeleteRealCustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteLegalCustomerServlet</servlet-name>
        <url-pattern>/DeleteLegalCustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateLegalCustomerServlet</servlet-name>
        <url-pattern>/UpdateLegalCustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateRealCustomerServlet</servlet-name>
        <url-pattern>/UpdateRealCustomerServlet</url-pattern>
    </servlet-mapping>
</web-app>
